NODE_ENV=development
PORT=5000

# Supabase Configuration
# Allez dans Settings > API pour trouver ces valeurs :
# - Project URL
# - service_role key (cliquez sur "Reveal")
SUPABASE_URL=https://votre-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=votre-service-role-key-ici

# Mot de passe de la base de données Supabase
# C'est le mot de passe que vous avez choisi lors de la création du projet
SUPABASE_DB_PASSWORD=votre-mot-de-passe-de-creation-du-projet

# JWT Configuration
JWT_SECRET=votre-secret-jwt-super-securise-changez-moi-en-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=votre-refresh-token-secret
JWT_REFRESH_EXPIRES_IN=7d

# QR Code Secret
QR_SECRET=votre-secret-qr-code-changez-moi-en-production

# CORS
CORS_ORIGIN=http://localhost:5173
